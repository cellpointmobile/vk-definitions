{
    "github": {
        "directdownload": [
            {
                "Cmd": "docker-compose",
                "VersionArg": "version",
                "VersionRegexp": "docker-compose version (.+?),",
                "GithubOwner": "docker",
                "GithubRepo": "compose",
                "DownloadURL": "https://github.com/docker/compose/releases/download/{VERSION}/docker-compose-Linux-x86_64"
            },
            {
                "Cmd": "helmfile",
                "VersionArg": "--version",
                "VersionRegexp": "version (v.+?)\n",
                "GithubOwner": "roboll",
                "GithubRepo": "helmfile",
                "ReleaseName": "helmfile_linux_amd64"
            },
            {
                "Cmd": "img",
                "VersionArg": "version",
                "VersionRegexp": "version.+?(v.+?)\n",
                "GithubOwner": "genuinetools",
                "GithubRepo": "img",
                "ReleaseName": "img-linux-amd64"
            },
            {
                "Cmd": "kops",
                "VersionArg": "version",
                "VersionRegexp": "Version (.+?) ",
                "GithubOwner": "kubernetes",
                "GithubRepo": "kops",
                "DownloadURL": "https://github.com/kubernetes/kops/releases/download/{VERSION}/kops-linux-amd64"
            },
            {
                "Cmd": "kubedb",
                "VersionArg": "version",
                "VersionRegexp": "Version = (.+?)\n",
                "GithubOwner": "kubedb",
                "GithubRepo": "cli",
                "DownloadURL": "https://github.com/kubedb/cli/releases/download/{VERSION}/kubedb-linux-amd64"
            },
            {
                "Cmd": "kustomize",
                "VersionArg": "version",
                "VersionRegexp": "KustomizeVersion:(.+?) ",
                "GithubOwner": "kubernetes-sigs",
                "GithubRepo": "kustomize",
                "ReleaseName": "kustomize_{VERSION}_linux_amd64"
            },
            {
                "Cmd": "minikube",
                "VersionArg": "version",
                "VersionRegexp": "version: (v.+?)\n",
                "GithubOwner": "kubernetes",
                "GithubRepo": "minikube",
                "ReleaseName": "minikube-linux-amd64"
            },
            {
                "Cmd": "stern",
                "VersionArg": "--version",
                "VersionRegexp": "version (.+?)\n",
                "GithubOwner": "wercker",
                "GithubRepo": "stern",
                "DownloadURL": "https://github.com/wercker/stern/releases/download/{VERSION}/stern_linux_amd64"
            },
            {
                "Cmd": "terraform-docs",
                "VersionArg": "--version",
                "VersionRegexp": "(.+?)\n",
                "GithubOwner": "segmentio",
                "GithubRepo": "terraform-docs",
                "ReleaseName": "terraform-docs-{VVERSION}-linux-amd64"
            },
            {
                "Cmd": "vk",
                "VersionArg": "version",
                "VersionRegexp": "Version: (.+?)\n",
                "GithubOwner": "cellpointmobile",
                "GithubRepo": "vk",
                "ReleaseName": "vk_{VERSION}_linux_x86_64"
            }
        ],
        "untarfile": [
            {
                "Cmd": "dive",
                "VersionArg": "version",
                "VersionRegexp": "dive (.+?)\n",
                "GithubOwner": "wagoodman",
                "GithubRepo": "dive",
                "ReleaseName": "dive_{VERSION}_linux_amd64.tar.gz",
                "PreRelease": true,
                "Filename": "dive"
            },
            {
                "Cmd": "fly",
                "VersionArg": "--version",
                "VersionRegexp": "(.+?)\n",
                "GithubOwner": "concourse",
                "GithubRepo": "concourse",
                "ReleaseName": "fly-linux-amd64.tgz",
                "Filename": "fly"
            },
            {
                "Cmd": "helm",
                "VersionArg": "version --client",
                "VersionRegexp": "SemVer:\"(.+?)\"",
                "GithubOwner": "kubernetes",
                "GithubRepo": "helm",
                "DownloadURL": "https://storage.googleapis.com/kubernetes-helm/helm-{VVERSION}-linux-amd64.tar.gz",
                "Filename": "linux-amd64/helm"
            },
            {
                "Cmd": "k8sec",
                "VersionArg": "version",
                "VersionRegexp": "version (v.+?),",
                "GithubOwner": "dtan4",
                "GithubRepo": "k8sec",
                "ReleaseName": "k8sec-{VVERSION}-linux-amd64.tar.gz",
                "Filename": "linux-amd64/k8sec"
            },
            {
                "Cmd": "kubespy",
                "VersionArg": "version",
                "VersionRegexp": "(.+?)\n",
                "GithubOwner": "pulumi",
                "GithubRepo": "kubespy",
                "ReleaseName": "kubespy-linux-amd64.tar.gz",
                "Filename": "releases/kubespy-linux-amd64/kubespy"
            },
            {
                "Cmd": "velero",
                "VersionArg": "version",
                "VersionRegexp": "Version: (v.+?)\n",
                "GithubOwner": "heptio",
                "GithubRepo": "velero",
                "ReleaseName": "velero-{VVERSION}-linux-amd64.tar.gz",
                "Filename": "velero"
            }
        ],
        "unzipfile": [
            {
                "Cmd": "tflint",
                "VersionArg": "--version",
                "VersionRegexp": "version (.+?)\n",
                "GithubOwner": "wata727",
                "GithubRepo": "tflint",
                "ReleaseName": "tflint_linux_amd64.zip",
                "Filename": "tflint"
            }
        ]
    },
    "hashicorp": [
        {
            "Cmd": "packer",
            "VersionArg": "-v",
            "VersionRegexp": "(.+?)\n"
        },
        {
            "Cmd": "terraform",
            "VersionArg": "-v",
            "VersionRegexp": "Terraform (v.+?)\n"
        },
        {
            "Cmd": "vagrant",
            "VersionArg": "-v",
            "VersionRegexp": "Vagrant (.+?)\n"
        }
    ]
}